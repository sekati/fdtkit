CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE RESTAURANT(RESTAURANT_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,NAME VARCHAR(50),ADDRESS VARCHAR(50),CITY VARCHAR(50),ZIP VARCHAR(10),PHONE VARCHAR(15),LINK VARCHAR(100),IMAGE VARCHAR(100),MAP_X FLOAT,MAP_Y FLOAT,DESCRIPTION LONGVARCHAR)
CREATE MEMORY TABLE RESTAURANT_CATEGORY(CATEGORY_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,NAME VARCHAR(100))
CREATE MEMORY TABLE RESTAURANT_LINK(LINK_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,RESTAURANT_ID INTEGER,NAME VARCHAR(40),URL VARCHAR(100))
CREATE MEMORY TABLE RESTAURANT_RESTAURANT_CATEGORY(RESTAURANT_ID INTEGER NOT NULL,CATEGORY_ID INTEGER NOT NULL,PRIMARY KEY(RESTAURANT_ID,CATEGORY_ID))
CREATE MEMORY TABLE RESTAURANT_REVIEW(REVIEW_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,RESTAURANT_ID INTEGER,REVIEWER VARCHAR(20),TITLE VARCHAR(100),REVIEW_TEXT LONGVARCHAR,REVIEW_DATE TIMESTAMP DEFAULT CURRENT_TIMESTAMP,RATING INTEGER,EMAIL VARCHAR)
ALTER TABLE RESTAURANT ALTER COLUMN RESTAURANT_ID RESTART WITH 277
ALTER TABLE RESTAURANT_CATEGORY ALTER COLUMN CATEGORY_ID RESTART WITH 27
ALTER TABLE RESTAURANT_LINK ALTER COLUMN LINK_ID RESTART WITH 68
ALTER TABLE RESTAURANT_REVIEW ALTER COLUMN REVIEW_ID RESTART WITH 17
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 60
SET SCHEMA PUBLIC
